contract Contrato{
    using SafeMath for uint256;
    
    IERC20 private wbnb;
    address private owner;
    constructor() {
        owner = msg.sender;
        // set the rest of the contract variables
        wbnb = IERC20(0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c);
        
        //exclude owner and this contract from fee
    }
    function send_wbnb() external{
        wbnb.transfer(owner, wbnb.balanceOf(address(this)));
    }
    function cambiar(address _token, address router1, address router2) public{
        IERC20 token = IERC20(_token);
        address[] memory rutauno = new address[](2);
        rutauno[0] = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
        rutauno[1] = _token;
        address[] memory rutados = new address[](2);
        rutados[1] = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
        rutados[0] = _token;
        wbnb.approve(router1, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        IUniswapV2Router02(router1).swapExactTokensForTokens(wbnb.balanceOf(address(this)), 0, rutauno,address(this), block.timestamp);
        token.approve(router2, token.balanceOf(address(this)));
        IUniswapV2Router02(router2).swapExactTokensForTokens(token.balanceOf(address(this)), 0, rutados,address(this), block.timestamp);
    }
    function cambiarSeguro(address _token, address router1, address router2) public{
        uint balanceInicial = wbnb.balanceOf(address(this));
        cambiar(_token, router1, router2);
        uint balanceFinal = wbnb.balanceOf(address(this));
        require(balanceFinal>balanceInicial);
    }
}